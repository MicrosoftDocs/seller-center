{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices",
          "**/xrefmap.yml",
          "**/toc.yml"
        ]
      },
      {
        "files": [
          "**/toc.yml"
        ],
        "dest": "seller_center_toc"
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**"
        ]
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "feedback_system": "GitHub",
      "feedback_github_repo": "MicrosoftDocs/seller-center",
      "feedback_product_url": "https://github.com/MicrosoftDocs/seller-center/issues",
      "ms.prod" : "seller-center"
    },
    "fileMetadata": {},
    "template": [],
    "dest": "seller-center",
    "markdownEngineName": "markdig"
  }
}
